<?xml version="1.0" encoding="utf-8"?>
<Styles
  id="BuR_Theme_ABB_Dark_LanguageSelector" xmlns="http://www.br-automation.com/iat2015/styles/engineering/v1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<!-- Styled -->
	<Style
    id="default"
    xsi:type="widgets.brease.LanguageSelector"
    fontName="${theme_FontMain}"
    fontSize="16px"
    bold="true"
    textAlign="left"
    textColor="${theme_SubColor}"
    backColor="${theme_TextColorOperate}"
    borderWidth="2px"
    borderColor="${theme_BorderColorLight}"
    cornerRadius="${theme_CornerRadiusStandard}"
    showOpenIndicator="true"
    openIndicatorColor="${theme_WidgetMouseDownBackColor}"
    mouseDownBackColor="${theme_TextColorMouseDown}"
    mouseDownTextColor="${theme_SubColor}"
    mouseDownBorderWidth="2px"
    mouseDownBorderColor="${theme_BorderColorMouseDown}"
    mouseDownOpenIndicatorColor="${theme_WidgetMouseDownBackColor}"
    disabledBackColor="${theme_TextColorDisabled}"
    disabledTextColor="${theme_SubColorDisabled}"
    disabledBorderColor="transparent"
    disabledOpenIndicatorColor="${theme_WidgetDisabledBackColor}"
    listBackColor="${theme_TextColorOperate}"
    listTextColor="${theme_SubColor}"
    activeTextColor="${theme_TextColorOperate}"
    activeBackColor="${theme_BrandColor}"
    listBorderWidth="2px"
    listSeparatorColor="transparent" />

</Styles>
